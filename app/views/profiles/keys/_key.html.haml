%li
  = link_to profile_key_path(key) do
    %strong= key.title
  %span
    (#{key.fingerprint})
  %span.cgray
    added #{time_ago_with_tooltip(key.created_at)}

  = link_to '删除', profile_key_path(key), data: { confirm: '您确定要删除吗?'}, method: :delete, class: "btn btn-small btn-remove delete-key pull-right"
